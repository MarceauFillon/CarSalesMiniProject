@model CarSalesMiniProject.ViewModels.CarListViewModel
@using CarSalesMiniProject.ViewModels

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div>
    @foreach (CarViewModel car in Model.CarList)
    {
        <div class="row">
            <div class="col-2">
                @car.Make
            </div>
            <div class="col-2">
                @car.Model
            </div>
            <div class="col-2">
                @car.AddDate
            </div>
        </div>
    }
</div>
